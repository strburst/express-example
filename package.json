{
  "name": "express-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "gulp": "gulp",
    "start": "node app.js"
  },
  "browserslist": [
    "> 1%"
  ],
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-unused-vars": [
        "warn",
        {
          "vars": "local",
          "args": "after-used"
        }
      ]
    }
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "^2.6.0",
    "express": "~4.14.0",
    "morgan": "~1.7.0",
    "pug": "~2.0.0-beta6",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "^2.18.6",
    "del": "^2.2.2",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-live-server": "0.0.30",
    "gulp-load-plugins": "^1.4.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.0",
    "nodemon": "^1.11.0"
  }
}
